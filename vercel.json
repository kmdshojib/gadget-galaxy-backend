{
    "version": 2,
    "builds": [
        {
            "src": "dist/app/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/app/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app/app.js"
        }
    ]
}